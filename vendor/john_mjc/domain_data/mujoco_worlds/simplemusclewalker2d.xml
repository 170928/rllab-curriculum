<mujoco model="walker2d">
  <compiler inertiafromgeom="true" angle="degree" coordinate="global"/>
  <option timestep="0.005" remotecontact="true" expdist="100.0"/>
  <default>
   <joint islimited='true' damping='.1' armature='0.01'  compliance='0.01' timeconst='0.01'/>
   <geom contype='1' conaffinity='0' condim='3' friction='.7 .1 .1' rgba='0.8 0.6 .4 1' density='200' mindist='0.1' compliance='0.01'  timeconst='0.01'/>
 </default>

 <body>
  <geom name='floor' pos='0 0 0' size='40 40 40' type='plane' conaffinity='1'  rgba='0.8 0.9 0.8 1' condim='3'/>

  <body name="torso" pos="0 0 1.25">
    <joint name='rootz' type='slide' axis='0 0 1' pos='0 0 0' islimited='false' damping='0' armature='0' stiffness='0' ref="1.25"/>
    <joint name='rootx' type='slide' axis='1 0 0' pos='0 0 0' islimited='false' damping='0' armature='0' stiffness='0'/>
    <joint name='rooty' type='hinge' axis='0 1 0' pos='0 0 1.25' islimited='false' damping='0' armature='0' stiffness='0'/>

    <geom name="torso_geom" type="capsule" fromto="0 0 1.45 0 0 1.05" size="0.05" friction="0.9"/>

    <body name="thigh" pos="0 0 1.05">
      <joint name="thigh_joint" type="hinge" pos="0 0 1.05" axis="0 -1 0" range="-150 0"/>
      <geom name="thigh_geom" type="capsule" fromto="0 0 1.05 0 0 0.6" size="0.05" friction="0.9"/>
      <site name="tight_muscle_site" pos="0 0 0.85"/>
      <body name="leg" pos="0 0 0.35">
        <joint name="leg_joint" type="hinge" pos="0 0 0.6" axis="0 -1 0" range="-150 0"/>
        <site name="leg_front_site" pos="0.1 0.0 0.6"/>
        <geom name="leg_joint_front_site" type="cylinder" size="0.01" fromto="0.1 0.1 0.6 0.1 -0.1 0.6"/>
        <site name="leg_joint_site" pos="0 0 0.6"/>
        <geom name="leg_joint_back_site" type="cylinder" size="0.01" fromto="-0.1 0.1 0.6 -0.1 -0.1 0.6"/>
        <site name="leg_back_site" pos="-0.1 0.0 0.6"/>
        <site name="leg_muscle_site" pos="0 0 0.35"/>
        <geom name="leg_geom" type="capsule" fromto="0 0 0.6 0 0 0.1" size="0.04" friction="0.9"/>
        <body name="foot" pos="0.2/2 0 0.1">
          <joint  name="foot_joint" type="hinge" pos="0 0 0.1" axis="0 -1 0" range="-45 45"/>
          <geom name="foot_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.1 0 0.1 0.1"/>
          <site name="foot_front_joint_site" pos="0.05 0 0.1"/>
          <site name="foot_back_joint_site" pos="-0.05 0 0.1"/>
          <geom name="foot_geom" type="capsule" fromto="-0.0 0 0.1 0.2 0 0.1" size="0.06" friction="0.9"/>
        </body>
      </body>          
    </body>

    <!-- copied and then replace thigh->thigh_left, leg->leg_left, foot->foot_right -->
    <body name="thigh_left" pos="0 0 1.05">
      <joint name="thigh_left_joint" type="hinge" pos="0 0 1.05" axis="0 -1 0" range="-150 0"/>
      <geom name="thigh_left_geom" type="capsule" fromto="0 0 1.05 0 0 0.6" size="0.05" friction="0.9" rgba=".7 .3 .6 1"/>
      <site name="left_tight_muscle_site" pos="0 0 0.85"/>
      <body name="leg_left" pos="0 0 0.35">
        <joint name="leg_left_joint" type="hinge" pos="0 0 0.6" axis="0 -1 0" range="-150 0"/>
        <geom name="left_leg_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.6 0 0.1 0.6"/>
        <site name="left_leg_front_site" pos="0.1 0.0 0.6"/>
        <geom name="left_leg_joint_front_site" type="cylinder" size="0.01" fromto="0.05 0.1 0.6 0.05 -0.1 0.6"/>
        <site name="left_leg_joint_site" pos="0 0 0.6"/>
        <geom name="left_leg_joint_back_site" type="cylinder" size="0.01" fromto="-0.05 0.1 0.6 -0.05 -0.1 0.6"/>
        <site name="left_leg_back_site" pos="-0.1 0.0 0.6"/>
        <site name="left_leg_muscle_site" pos="0 0 0.35"/>        
        <geom name="leg_left_geom" type="capsule" fromto="0 0 0.6 0 0 0.1" size="0.04" friction="0.9" rgba=".7 .3 .6 1"/>
        <body name="foot_left" pos="0.2/2 0 0.1">
          <joint  name="foot_left_joint" type="hinge" pos="0 0 0.1" axis="0 -1 0" range="-45 45"/>
          <geom name="left_foot_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.1 0 0.1 0.1"/>
          <site name="left_foot_front_joint_site" pos="0.05 0 0.1"/>
          <site name="left_foot_back_joint_site" pos="-0.05 0 0.1"/>
          <geom name="foot_left_geom" type="capsule" fromto="-0.0 0 0.1 0.2 0 0.1" size="0.06" friction="0.9" rgba=".7 .3 .6 1"/>
        </body>
      </body>          
    </body>

  </body>
</body>

	<tendon>
    	<spatial name="M1" islimited="true" range="0 0.45">
    		<site site="foot_front_joint_site"/>
    		<site site="leg_muscle_site"/>
    	</spatial>
    	<spatial name="M2" islimited="true" range="0 0.45">
    		<site site="foot_back_joint_site"/>
    		<site site="leg_muscle_site"/>
    	</spatial>
      <spatial name="M3" islimited="true" range="0 0.65">
        <site site="leg_muscle_site"/>
        <geom geom="leg_joint_front_site" sidesite="leg_front_site"/>
        <site site="tight_muscle_site"/>
      </spatial>
      <spatial name="M4" islimited="true" range="0 0.65">
        <site site="leg_muscle_site"/>
        <geom geom="leg_joint_back_site" sidesite="leg_back_site"/>
        <site site="tight_muscle_site"/>
      </spatial>
      <spatial name="left_M1" islimited="true" range="0 0.45">
        <site site="left_foot_front_joint_site"/>
        <site site="left_leg_muscle_site"/>
      </spatial>
      <spatial name="left_M2" islimited="true" range="0 0.45">
        <site site="left_foot_back_joint_site"/>
        <site site="left_leg_muscle_site"/>
      </spatial>
      <spatial name="left_M3" islimited="true" range="0 0.65">
        <site site="left_leg_muscle_site"/>
        <geom geom="left_leg_joint_front_site" sidesite="left_leg_front_site"/>
        <site site="left_tight_muscle_site"/>
      </spatial>
      <spatial name="left_M4" islimited="true" range="0 0.65">
        <site site="left_leg_muscle_site"/>
        <geom geom="left_leg_joint_back_site" sidesite="left_leg_back_site"/>
        <site site="left_tight_muscle_site"/>
      </spatial>
    </tendon>

<actuator>
  <!-- <motor joint="torso_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <motor joint="thigh_joint" ctrlrange="-150.0 150.0" isctrllimited="true"/>
  <!-- <motor joint="leg_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="foot_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <motor joint="thigh_left_joint" ctrlrange="-150.0 150.0" isctrllimited="true"/>
  <!-- <motor joint="leg_left_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="foot_left_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="finger2_rot" ctrlrange="-20.0 20.0" isctrllimited="true"/> -->
  <muscle dyntype="muscle" trntype="tendon" tendon="M1" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="M2" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="M3" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="M4" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_M1" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_M2" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_M3" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_M4" gaintype="muscle" biastype="muscle" ctrlrange="-10 10" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.2" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="1.0"/>
</actuator>

</mujoco>