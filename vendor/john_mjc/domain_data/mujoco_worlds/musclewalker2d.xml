<!-- new -->
<mujoco model="walker2d">
  <compiler inertiafromgeom="true" angle="degree" coordinate="global"/>
  <option timestep="0.005" remotecontact="true" expdist="50.0"/>
  <default>
   <joint islimited='true' damping='.1' armature='0.01'  compliance='0.01' timeconst='0.01'/>
   <geom contype='1' conaffinity='0' condim='3' friction='.7 .1 .1' rgba='0.8 0.6 .4 1' density='1000' mindist='0.04' compliance='0.01'  timeconst='0.01'/>
 </default>

 <body>
  <geom name='floor' pos='0 0 0' size='40 40 40' type='plane' conaffinity='1'  rgba='0.8 0.9 0.8 1' condim='3'/>

  <body name="torso" pos="0 0 1.25">
    <joint name='rootz' type='slide' axis='0 0 1' pos='0 0 0' islimited='false' damping='0' armature='0' stiffness='0' ref="1.25"/>
    <joint name='rootx' type='slide' axis='1 0 0' pos='0 0 0' islimited='false' damping='0' armature='0' stiffness='0'/>
    <joint name='rooty' type='hinge' axis='0 1 0' pos='0 0 1.25' islimited='false' damping='0' armature='0' stiffness='0'/>

    <geom name="torso_geom" type="capsule" fromto="0 0 1.45 0 0 1.05" size="0.05" friction="0.9"/>

    <body name="thigh" pos="0 0 1.05">
      <joint name="thigh_joint" type="hinge" pos="0 0 1.05" axis="0 -1 0" range="-150 0"/>
      <geom name="thigh_geom" type="capsule" fromto="0 0 1.05 0 0 0.6" size="0.05" friction="0.9"/>
      <site name="tight_muscle_site" pos="0 0 0.8"/>
      <body name="leg" pos="0 0 0.35">
        <joint name="leg_joint" type="hinge" pos="0 0 0.6" axis="0 -1 0" range="-150 0"/>
        <geom name="leg_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.6 0 0.1 0.6"/>
        <site name="leg_front_site" pos="1.0 0.0 0.6"/>
        <site name="leg_joint_site" pos="0 0 0.6"/>
        <site name="leg_upper_muscle_site" pos="0 0 0.5"/>
        <site name="leg_lower_muscle_site" pos="0 0 0.3"/>
        <geom name="leg_geom" type="capsule" fromto="0 0 0.6 0 0 0.1" size="0.04" friction="0.9"/>
        <body name="foot" pos="0.2/2 0 0.1">
          <joint  name="foot_joint" type="hinge" pos="0 0 0.1" axis="0 -1 0" range="-45 45"/>
          <geom name="foot_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.1 0 0.1 0.1"/>
          <site name="foot_joint_site" pos="0 0 0.1"/>
          <site name="foot_muscle_site" pos="0.15 0 0.1"/>
          <site name="foot_back_muscle_site" pos="-0.1 0 0.1"/>
          <geom name="foot_geom" type="capsule" fromto="-0.0 0 0.1 0.2 0 0.1" size="0.06" friction="0.9"/>
        </body>
      </body>          
    </body>

    <!-- copied and then replace thigh->thigh_left, leg->leg_left, foot->foot_right -->
    <body name="thigh_left" pos="0 0 1.05">
      <joint name="thigh_left_joint" type="hinge" pos="0 0 1.05" axis="0 -1 0" range="-150 0"/>
      <geom name="thigh_left_geom" type="capsule" fromto="0 0 1.05 0 0 0.6" size="0.05" friction="0.9" rgba=".7 .3 .6 1"/>
      <site name="left_tight_muscle_site" pos="0 0 0.7"/>
      <body name="leg_left" pos="0 0 0.35">
        <geom name="left_leg_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.6 0 0.1 0.6"/>
        <site name="left_leg_front_site" pos="1.0 0.0 0.6"/>     
        <joint name="leg_left_joint" type="hinge" pos="0 0 0.6" axis="0 -1 0" range="-150 0"/>
        <site name="left_leg_joint_site" pos="0 0 0.6"/>
        <site name="left_leg_upper_muscle_site" pos="0 0 0.5"/>
        <site name="left_leg_lower_muscle_site" pos="0 0 0.3"/>
        <geom name="leg_left_geom" type="capsule" fromto="0 0 0.6 0 0 0.1" size="0.04" friction="0.9" rgba=".7 .3 .6 1"/>
        <body name="foot_left" pos="0.2/2 0 0.1">

          <joint  name="foot_left_joint" type="hinge" pos="0 0 0.1" axis="0 -1 0" range="-45 45"/>
          <geom name="left_foot_cylinder" type="cylinder" size="0.01" fromto="0 -0.1 0.1 0 0.1 0.1"/>
          <site name="left_foot_joint_site" pos="0.0 0 0.1"/>
          <site name="left_foot_muscle_site" pos="0.15 0 0.1"/>
          <site name="left_foot_back_muscle_site" pos="-0.1 0 0.1"/>
          <geom name="foot_left_geom" type="capsule" fromto="-0.0 0 0.1 0.2 0 0.1" size="0.06" friction="0.9" rgba=".7 .3 .6 1"/>
        </body>
      </body>          
    </body>

  </body>
</body>

	<tendon>
    	<spatial name="SOL" islimited="true" range="0 0.4">
    		<site site="leg_lower_muscle_site"/>
    		<geom geom="foot_cylinder" sidesite="foot_back_muscle_site"/>
    		<site site="foot_muscle_site"/>
    	</spatial>
    	<spatial name="TA" islimited="true" range="0 0.2">
    		<site site="leg_lower_muscle_site"/>
    		<site site="foot_muscle_site"/>
    	</spatial>
      <spatial name="VER" islimited="true" range="0 0.35">
        <site site="leg_lower_muscle_site"/>
        <site site="foot_joint_site"/>
        <site site="foot_back_muscle_site"/>
      </spatial> 
      <spatial name="left_VER" islimited="true" range="0 0.35">
        <site site="left_leg_lower_muscle_site"/>
        <site site="left_foot_joint_site"/>
        <site site="left_foot_back_muscle_site"/>
      </spatial>
    	<spatial name="left_SOL" islimited="true" range="0 0.4">
    		<site site="left_leg_lower_muscle_site"/>
        <geom geom="left_foot_cylinder" sidesite="left_foot_back_muscle_site"/>
    		<site site="left_foot_muscle_site"/>
    	</spatial>
    	<spatial name="left_TA" islimited="true" range="0 0.2">
    		<site site="left_leg_lower_muscle_site"/>
    		<site site="left_foot_muscle_site"/>
    	</spatial>
       	<spatial name="VAS" islimited="true" range="0 0.6">
    		<site site="tight_muscle_site"/>
    		<geom geom="leg_cylinder" sidesite="leg_front_site"/>
    		<site site="leg_lower_muscle_site"/>
    	</spatial>
    	<spatial name="left_VAS" islimited="true" range="0 0.6">
    		<site site="left_tight_muscle_site"/>
    		<geom geom="left_leg_cylinder" sidesite="left_leg_front_site"/>
    		<site site="left_leg_lower_muscle_site"/>
    	</spatial>
      <spatial name="GAS" islimited="true" range="0 0.8">
        <site site="tight_muscle_site"/>
        <geom geom="leg_cylinder" sidesite="leg_front_site"/>
        <site site="foot_muscle_site"/>
      </spatial>
      <spatial name="left_GAS" islimited="true" range="0 0.8">
        <site site="left_tight_muscle_site"/>
        <geom geom="left_leg_cylinder" sidesite="left_leg_front_site"/>
        <site site="left_foot_muscle_site"/>
      </spatial>
    </tendon>

<actuator>
  <!-- <motor joint="torso_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <motor joint="thigh_joint" ctrlrange="-150.0 150.0" isctrllimited="true"/>
  <!-- <motor joint="leg_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="foot_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <motor joint="thigh_left_joint" ctrlrange="-150.0 150.0" isctrllimited="true"/>
  <!-- <motor joint="leg_left_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="foot_left_joint" ctrlrange="-100.0 100.0" isctrllimited="true"/> -->
  <!-- <motor joint="finger2_rot" ctrlrange="-20.0 20.0" isctrllimited="true"/> -->
  <muscle dyntype="muscle" trntype="tendon" tendon="VER" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_VER" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="SOL" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="TA" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_SOL" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_TA" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="VAS" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_VAS" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="GAS" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
  <muscle dyntype="muscle" trntype="tendon" tendon="left_GAS" gaintype="muscle" biastype="muscle" ctrlrange="-1 1" forcerange="-20 20" lminrel="0.5" lmaxrel="1" widthrel="0.1" vmaxrel="1.0" tact="1.0" tdeact="0.5" fmax="200.0"/>
</actuator>

</mujoco>