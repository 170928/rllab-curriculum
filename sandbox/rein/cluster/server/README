docker build -t rein/rllab-q . && docker push rein/rllab-q

# container takes on same ports as host by using --net="host", uses same net interface as host
# -i = interactive, -t = tty, prompt output
sudo docker pull rein/rllab-q && sudo docker run -t -i --net="host" rein/rllab-q

